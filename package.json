{
  "name": "@fec/eleventy-plugin-remark",
  "version": "1.0.0",
  "description": "Use remark to compile Markdown in Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "eleventy-plugin",
    "eleventy",
    "remark",
    "markdown"
  ],
  "author": "Florian Eckerstorfer <florian@eckerstorfer.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.7.4",
    "@types/node": "^12.12.14",
    "jest": "^24.9.0",
    "remark": "^11.0.2",
    "remark-html": "^10.0.0"
  },
  "peerDependencies": {
    "remark": "^11.0.2",
    "remark-html": "^10.0.0"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,ts}", ".eleventy.js"],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testRegex": "/__tests__/.*\\.spec\\.[jt]s$"
  }
